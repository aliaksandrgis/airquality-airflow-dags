# This file is read by Docker Compose for ${VAR} interpolation in docker-compose.yml.
# Copy it to `.env` and adjust values.

# Airflow container user (uid) for volume permissions.
AIRFLOW_UID=50000

# Required for DockerOperator: this gid must match the host `docker` group gid so the
# Airflow container user can access `/var/run/docker.sock`.
# Find it with: `getent group docker | cut -d: -f3`
DOCKER_GID=999

